clbo:DatenqualitätsnormJ a owl:Class;
    rdfs:label "data quality standard J"@en, "Daten qualitat norm J"@du;
    rdfs:isDefinedBy <https://climatebowl.data.dice-research.org/ontology/> ;
    rdfs:comment "data quality standard J" .

clbo:hatInformationsunsicherheit a owl:Property;
    rdfs:label "has information insecurity"@en, "hat informationsunsicherheit"@du;
    rdfs:domain clbo:Datenqualitatsbewertung;
    rdfs:range clbo:DatenqualitatsnormJ;
    rdfs:isDefinedBy <https://climatebowl.data.dice-research.org/ontology/> ;
    rdfs:comment "has information insecurity" .

clbo:informationsunsicherheit a owl:Property;
    rdfs:label "information insecurity"@en, "informationsunsicherheit"@du;
    rdfs:domain clbo:DatenqualitatsnormJ;
    rdfs:range xsd:string;
    rdfs:isDefinedBy <https://climatebowl.data.dice-research.org/ontology/> ;
    rdfs:comment "information insecurity" .


clbo:istVerflichtend a owl:Property;
    rdfs:label "Is mandatory"@en, "Ist Verflichtend"@du;
    rdfs:domain clbo:DatenqualitatsbewertungA,
        clbo:DatenqualitätsnormB,
        clbo:DatenqualitätsnormC,
        clbo:DatenqualitätsnormD,
        clbo:DatenqualitätsnormE,
        clbo:DatenqualitätsnormF,
        clbo:DatenqualitätsnormG,
        clbo:DatenqualitätsnormH,
        clbo:DatenqualitätsnormI,
        clbo:DatenqualitätsnormJ ;
    rdfs:range xsd:boolean;
    rdfs:isDefinedBy <https://climatebowl.data.dice-research.org/ontology/> ;
    rdfs:comment "Is mandatory" .

# THGEmissionen Class
clbo:THGEmissionen a owl:Class;
    rdfs:label "THG Emissions"@en, "THG Emissionen"@du;
    rdfs:isDefinedBy <https://climatebowl.data.dice-research.org/ontology/> ;
    rdfs:comment "" .

clbo:hatEmissionsfaktor a owl:Property;
    rdfs:label "has Emission Factor"@en, ":hat Emissions faktor"@du;
    rdfs:domain clbo:Fluss, clbo:Eisenmetall, clbo:Nichteisenmetall, clbo:Kohlenstoffmodifikation, clbo:Polymer ;
    rdfs:range clbo:THGEmissionen;
    rdfs:isDefinedBy <https://climatebowl.data.dice-research.org/ontology/> ;
    rdfs:comment "" .

